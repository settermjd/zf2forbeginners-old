<tr>
    <td> <a href="<?php print $this->url(
            'feeds/manage', array('id' => $this->feedId));
        ?>" title="Update Feed"><?php print $this->feedId; ?></a></td>
    <td><?php echo $this->dateFormat(
            new \DateTime($this->feedDate . ' ' . $this->feedTime),
            IntlDateFormatter::MEDIUM,
            IntlDateFormatter::MEDIUM
        ); ?></td>
    <td><?php echo $this->feedAmount; ?></td>
    <td><?php echo $this->numberFormat(
            $this->feedTemperature,
            NumberFormatter::DECIMAL,
            NumberFormatter::TYPE_DEFAULT
        ); ?></td>
    <td>
        <?php if (strlen($this->feedNotes) > 100) : ?>
            <?php echo substr($this->feedNotes, 0, 100) . '...'; ?>
        <?php else : ?>
            <?php echo $this->feedNotes; ?>
        <?php endif; ?>
    </td>
    <td style="text-align: center;">
        <a href="<?php print $this->url(
            'feeds/delete', array('id' => $this->feedId));
        ?>" title="Delete Feed"><?php print $this->translate('delete'); ?></a>
    </td>
</tr>